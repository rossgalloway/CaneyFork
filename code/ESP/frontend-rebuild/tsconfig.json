{
  "compilerOptions": {
    "outDir": "./build/",
    "noImplicitAny": false,
    "module": "esnext",
    "moduleResolution": "node",
    "target": "es2018",
    "jsx": "react-jsx",
    "baseUrl": "src",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "experimentalDecorators": true,
    "strictNullChecks": false,
    "skipLibCheck": true,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": false,
    "noEmit": true,
    "declaration": true,
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "allowUnreachableCode": false,
    "isolatedModules": true,
    "useDefineForClassFields": true,
    "noFallthroughCasesInSwitch": true,
    "useUnknownInCatchVariables": false,
    "noUnusedLocals": true
  },
  "include": [
    "src",
    "scripts",
    "src/declarations.d.ts"
, "unused/hooks/Guilds/useVoteSummary.ts", "unused/hooks/Guilds/useMenu.ts", "unused/components/primitives/Links/BlockExplorerLink.tsx", "unused/contexts/guilds/filters.tsx", "unused/contexts/guilds/guildAvailability.tsx", "unused/components/primitives/Links/BLockExplorerLink.test.tsx", "src/hooks/Guilds/conversions", "unused/utils/validation.test.ts"  ],
  "exclude": [
    "node_modules",
    "build",
    "cache",
    "artifacts",
    "public"
  ]
}
